,"source":"common/views/App.js","original":{"line":49,"column":50}},{"generated":{"line":76,"column":4},"source":"common/views/App.js","original":{"line":49,"column":50}},{"generated":{"line":76,"column":16},"source":"common/views/App.js","original":{"line":49,"column":62}},{"name":"ChatProtocol","generated":{"line":76,"column":18},"source":"common/views/App.js","original":{"line":49,"column":64}},{"generated":{"line":77,"column":0},"source":"common/views/App.js","original":{"line":49,"column":8}},{"generated":{"line":77,"column":4},"source":"common/views/App.js","original":{"line":47,"column":6}},{"generated":{"line":77,"column":5},"source":"common/views/App.js","original":{"line":43,"column":4}},{"generated":{"line":77,"column":6},"source":"common/views/App.js","original":{"line":42,"column":2}},{"generated":{"line":78,"column":0},"source":"common/views/App.js","original":{"line":53,"column":1}}],"sources":{"common/views/App.js":"/* eslint-env browser */\nimport React, { useState, useEffect } from 'react'\nimport EventEmitter from 'events'\nimport Header from './Header'\nimport Metrics from './Metrics'\nimport Chat from './Chat'\nimport { getOrCreatePeerId } from '../libs/peer-id'\nimport '../styles/index.css'\n\nexport default function App ({\n  createLibp2p,\n  ChatProtocol\n}) {\n  const [peerId, setPeerId] = useState(null)\n  const [libp2p, setLibp2p] = useState(null)\n  const [started, setStarted] = useState(false)\n  const eventBus = new EventEmitter()\n\n  /**\n   * Leverage use effect to act on state changes\n   */\n  useEffect(() => {\n    // If we don't have a PeerId, let's get or create it\n    // This will attempt to leverage localStorage so we can reuse our key\n    if (!peerId) {\n      console.info('Getting our PeerId')\n      getOrCreatePeerId().then(setPeerId)\n      return\n    }\n\n    // If the libp2p instance is not created, create it with our PeerId instance\n    if (!libp2p) {\n      ;(async () => {\n        console.info('Creating our Libp2p instance')\n        const node = await createLibp2p(peerId)\n        setLibp2p(node)\n        setStarted(true)\n      })()\n    }\n  })\n\n  return (\n    <div className='avenir flex flex-column h-100'>\n      <div className='flex-none'>\n        <Header started={started} />\n      </div>\n      <div className='flex h-100'>\n        <Metrics libp2p={libp2p} eventBus={eventBus} />\n        <Chat libp2p={libp2p} eventBus={eventBus} ChatProtocol={ChatProtocol} />\n      </div>\n    </div>\n  )\n}\n"},"lineCount":null}},"error":null,"hash":"f1a73bbbaaeaec53b29db5db83f7928c","cacheData":{"env":{}}}